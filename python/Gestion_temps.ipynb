    "class time_calcul:\n",
    "    \n",
    "    def __init__(self):\n",
    "        self.begin = 0\n",
    "        self.end = 0\n",
    "        \n",
    "    def start(self):\n",
    "        self.begin = time.time()\n",
    "        \n",
    "    def stop(self):\n",
    "        self.end = time.time()\n",
    "        return self.end - self.begin"
